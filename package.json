{
  "name": "nodejs-exercice-2",
  "version": "1.0.0",
  "description": "Nodejs exercice 2",
  "main": "index.js",
  "repository": "git@github.com:Epita-Expert/nodejs-exercice-2.git",
  "author": "Minaro <paturetromain@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/index.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/index.js\"",
    "test": "mocha",
    "prisma:generate": "npx prisma generate",
    "prisma:studio": "npx prisma studio",
    "prisma:seed": "npx ts-node prisma/seed.ts",
    "prisma:migrate": "npx prisma migrate dev --name init",
    "prisma:migrate:reset": "npx prisma migrate reset",
    "prisma:migrate:deploy": "npx prisma migrate deploy",
    "prisma:migrate:save": "npx prisma migrate save --experimental",
    "prisma:migrate:up": "npx prisma migrate up --experimental",
    "prisma:migrate:down": "npx prisma migrate down --experimental",
    "prisma:migrate:resolve": "npx prisma migrate resolve --experimental",
    "prisma:db:push": "npx prisma db push",
    "prisma:db:pull": "npx prisma db pull",
    "prisma:db:reset": "npx prisma db reset",
    "prisma:db:drop": "npx prisma db drop",
    "prisma:db:seed": "npx prisma db seed",
    "prisma:db:studio": "npx prisma db studio",
    "prisma:db:console": "npx prisma db console",
    "prisma:db:gui": "npx prisma db gui",
    "prisma:db:info": "npx prisma db info",
    "prisma:db:version": "npx prisma db version",
    "prisma:db:help": "npx prisma db help"
  },
  "dependencies": {
    "@prisma/client": "4.7.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "prisma": "^4.7.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.9",
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.20",
    "typescript": "^4.9.3"
  }
}
